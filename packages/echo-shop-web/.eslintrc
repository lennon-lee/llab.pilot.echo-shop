{
	"env": {
		"browser": true,
		"es2020": true
	},
	"extends": [
		"eslint:recommended",
		"airbnb",
		"plugin:prettier/recommended"
	],
	"parser": "babel-eslint",
	"rules": {
		"jsx-a11y/label-has-associated-control": 0,
		"react/jsx-filename-extension": 0,
		"react/jsx-props-no-spreading": "off",
		"no-underscore-dangle": 0,
		"no-console": 0
	}
}